    "@angular/platform-browser-dynamic": "^14.0.4",
    "@angular/router": "^14.0.4",
    "@ngrx/component-store": "14.0.1",
    "@ngrx/effects": "14.0.1",
    "@ngrx/entity": "14.0.1",
    "@ngrx/router-store": "14.0.1",
    "@ngrx/store": "14.0.1",
    "@nrwl/angular": "14.4.0",
    "rxjs": "~6.6.7",
    "tslib": "^2.0.0",
    "zone.js": "^0.11.5"
  },
